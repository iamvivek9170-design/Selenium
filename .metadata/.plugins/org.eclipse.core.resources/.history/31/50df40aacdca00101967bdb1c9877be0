	Thread.sleep(Duration.ofSeconds(5));
		
WebElement male = dr.findElement(By.xpath("[id=\"male\"]"));


//check method


//WebElement female = dr.findElement(By.xpath("[id=\"sports\"]"));


		WebElement sports = dr.findElement(By.xpath("[id=\"Music\"]"));


