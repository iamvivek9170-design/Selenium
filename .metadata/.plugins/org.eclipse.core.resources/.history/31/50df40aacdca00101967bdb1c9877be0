	public static void  check(WebElement c) {
		if(c.isSelected()) {
			c.click();
			System.out.println("element get click");
		}
	
	
	}
	
	public static void main(String[] args) throws InterruptedException {
	
		
		WebDriver dr = new ChromeDriver();
