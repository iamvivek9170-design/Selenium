package second;

import java.time.Duration;

import org.openqa.selenium.WebDriver;
import org.openqa.selenium.chrome.ChromeDriver;

public class Lab_1 {

	public static void main(String[] args) throws InterruptedException {
		
	
	WebDriver dr = new ChromeDriver();
	
	// opening lms website
	dr.get("https://school.moodledemo.net/");
	
	//fetching current url
	String sr = dr.getCurrentUrl();
	System.out.println(sr);
	
	// fetching current page title  
	String Title = dr.getTitle();
	System.out.println(Title);
	
	// fetching source code 
	String source = dr.getPageSource();
	System.out.println(source);
	
	
	dr.get("hhttps://school.moodledemo.net/login/index.php");
	Thread.sleep(Duration.ofSeconds(5));

	dr.navigate().back();
	
	System.out.println("Back to lms website");
	Thread.sleep(Duration.ofSeconds(5));
	
	System.out.println("Going farward to login page ");
	
	dr.navigate().forward();
	
	
	
	dr.quit();
	
	
	}
}
